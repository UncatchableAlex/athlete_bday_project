    "import pandas as pd\n",
    "df = pd.read_csv('Datasets/US_births_1994-2014.csv')\n",
    "df['births_by_year'] = df.groupby(['year'])['births'].transform('sum')\n",
    "df['density'] = df['births'] / df['births_by_year']\n",
    "df"
